    <costList>
      <Steel>55</Steel>
      <Cloth>15</Cloth>
    </costList>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>300</WorkToBuild>
      <Flammability>0.2</Flammability>
      <!--<MarketValue>38</MarketValue>-->
      <Mass>17.5</Mass>
      <Beauty>25</Beauty>
